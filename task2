#include <iostream>

using namespace std;

int main()
{
	int n;
	cin >> n; 
	
	int arr[n+2];
	for(int i = 1; i <= n; ++i)
	{
		cin >> arr[i]; 
	}
	
	int m; //число, которое необходимо вставить
	int k; //номер места
	cin >> m >> k;
	
	for( int i=n+1; i>k; i--){
	    arr[i] = arr[i-1];
	}
	arr[k] = m;
	
	for(int i = 1; i <= n+1 ; i++)
	{
		cout << arr[i] << " ";
	}
	
}
